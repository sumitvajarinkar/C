 }























#include<stdio.h>
#include<conio.h>
void main()
{
int a,b;
clrscr();
printf("Enter any two nos.=\n");
scanf("%d%d",&a,&b);
if(a>b)
{
 printf("No. a is greater\n");
 }
 else
 {
  printf("No. b is greater\n");
  }
getch();
}

